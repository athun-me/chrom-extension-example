{
    "manifest_version": 3,
    "name": "Data Display Extension",
    "version": "1.0",
    "description": "A simple Chrome extension to display input data.",
    "action": {
        "default_icon": "img/logo-48.png"
    },
    "background": {
        "service_worker": "dist/background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}