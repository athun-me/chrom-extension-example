{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "A simple Chrome extension.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "img/logo-48.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
